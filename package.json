{
  "name": "addvision",
  "homepage": "http://vadim4web.github.io/addvision/",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "deploy": "nuxi generate && gh-pages -d dist",
    "deploy_": "nuxt generate && gh-pages -d .output/public",
    "export:locales": "node scripts/export-locales.js",
    "prettify": "prettier --write \"**/*.{js,ts,vue}\"",
    "lint": "eslint \"**/*.{js,ts,vue}\" --fix",
    "format": "npm run prettify && npm run lint"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "^0.11.7",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^9.5.4",
    "@unhead/vue": "^2.0.9",
    "@vueuse/nuxt": "^13.6.0",
    "eslint": "^9.27.0",
    "gsap": "^3.13.0",
    "nuxt": "^3.17.3",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1",
    "vueperslides": "^3.6.0"
  },
  "devDependencies": {
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "gh-pages": "^6.3.0",
    "sass-embedded": "^1.89.0"
  }
}
